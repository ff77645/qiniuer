  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration":true,
    "moduleResolution":"nodenext"
  },
  "include": ["src/**/*", "src/types/**/*"]
}